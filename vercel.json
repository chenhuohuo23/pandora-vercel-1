{
  "builds": [
    {
      "src": "src/pandora_cloud/server.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source":"/v1/:path(.*)",
      "destination":"https://api.openai.com/v1/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "src/pandora_cloud/server.py"
    }
  ]
}
